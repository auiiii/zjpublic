<mapper namespace="com.zj.dao.BookDao">

    <select id="selectById" parameterType="int" resultType="Book">
        select * from oldbook where id = #{id}
    </select>

    <select id="selectAll" resultType="Book">
        select * from oldbook
    </select>

</mapper>